{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [


  
        {
             "args": [
            //     "-u",
            //     "tdd",
                 "--timeout",
                 "999999",
                 "--colors"
             ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Mocha Tests",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
            "request": "launch",
            "cwd": "${workspaceFolder}/generate",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "args": [
           //     "-u",
           //     "tdd",
                "--timeout",
                "999999",
                "--colors"
            ],
            "env": {"TAP_SNAPSHOT":"1"},
           "internalConsoleOptions": "openOnSessionStart",
           "name": "Mocha Transpile",
           "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
           "request": "launch",
           "cwd": "${workspaceFolder}/transpile",
           "skipFiles": [
               "<node_internals>/**"
           ],
           "type": "node"
       },
       {
            "args": [
           //     "-u",
           //     "tdd",
                "--timeout",
                "999999",
                "--colors"
            ],
           "internalConsoleOptions": "openOnSessionStart",
           "name": "Util Tests",
           "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
           "request": "launch",
           "cwd": "${workspaceFolder}/util",
           "skipFiles": [
               "<node_internals>/**"
           ],
           "type": "node"
       },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
            "program": "${workspaceFolder}/generate/src/run.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Tree",
            "skipFiles": [
                "<node_internals>/**"
            ],
//            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
            "program": "${workspaceFolder}/generate/tree.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Transpile",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/transpile",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
            "program": "${workspaceFolder}/transpile/src/transpile.ts",
            "args": ["${workspaceFolder}/demo/src/pages/AnimationDemo/AnimationDemo.swift"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "name": "Debug Mocha Transpile Tests",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "continueOnAttach": true,
            "autoAttachChildProcesses": false,
            "resolveSourceMapLocations": [
              "!**/node_modules/**",
              "!**/.vscode/extensions/hbenl.vscode-mocha-test-adapter-*/**"
            ],
            "skipFiles": [
              "<node_internals>/**"
            ]
          }
    ]
}