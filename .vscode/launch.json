{
     "version": "0.2.0",
    "configurations": [




  
        {
             "args": [
                 "--timeout",
                 "999999",
                 "--colors"
             ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Mocha Tests",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
            "request": "launch",
            "cwd": "${workspaceFolder}/generate",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            
            "env": {"TAP_SNAPSHOT":"1"},
           "internalConsoleOptions": "openOnSessionStart",
           "name": "Test Transpile",
           "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
           "request": "launch",
           "args": ["./test/transpile.test.ts"],
           "cwd": "${workspaceFolder}/transpile",
           "skipFiles": [
               "<node_internals>/**"
           ],
           "type": "node"
       },
       {
            "args": [
                "--timeout",
                "999999",
                "--colors"
            ],
           "internalConsoleOptions": "openOnSessionStart",
           "name": "Util Tests",
           "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
           "request": "launch",
           "cwd": "${workspaceFolder}/util",
           "skipFiles": [
               "<node_internals>/**"
           ],
           "type": "node"
       },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
            "program": "${workspaceFolder}/generate/src/run.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Tree",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/generate/tree.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Transpile",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/transpile",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
            "program": "${workspaceFolder}/transpile/src/transpile.ts",
            "args": ["${workspaceFolder}/demo/src/pages/AnimationDemo/AnimationDemo.swift"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "name": "Debug Mocha Transpile Tests",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "continueOnAttach": true,
            "autoAttachChildProcesses": false,
            "resolveSourceMapLocations": [
              "!**/node_modules/**",
              "!**/.vscode/extensions/hbenl.vscode-mocha-test-adapter-*/**"
            ],
            "skipFiles": [
              "<node_internals>/**"
            ]
          },
          {
            "type": "node",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
            "request": "launch",
            "name": "transpile.closure",
            "args": ["${workspaceFolder}/transpile/test/transpile.closure.test.ts"],
            "cwd": "${workspaceFolder}/transpile"
        },          {
            "type": "node",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
            "request": "launch",
            "name": "transpile.control",
            "args": ["${workspaceFolder}/transpile/test/transpile.control.test.ts"],
            "cwd": "${workspaceFolder}/transpile"
        }
, {
    "type": "node",
    "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
    "request": "launch",
    "name": "transpile.class",
    "args": ["${workspaceFolder}/transpile/test/transpile.class.test.ts"],
    "cwd": "${workspaceFolder}/transpile"
}
,
          {
            "type": "node",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
            "request": "launch",
            "name": "Run test (current)",
            "args": ["${workspaceFolder}/${relativeFile}"],
            "cwd": "${workspaceFolder}/transpile"
        }
    ]
}